function LeapYear(){
 var year = prompt("Enter Year to check");
  var cal1 = year;
  if(cal1%4===0)
  {
    if(cal1%4===0)
    {
     if(cal1%400===0)
     {
      return "leap year";
     }
     else
     {
      return "not a leap year";
     }
    }
    else
    {
     return "leap year";
    }
  }
 else
  {
  return "not a leap year";
 }
}

LeapYear();
